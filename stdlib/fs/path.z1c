m std.fs.path:1.0 ctx=128 caps=[]
#sym { ExtOption: EO, join: j, basename: bn, dirname: dn, extension: ext }

t EO = Some{ ext: Str } | None{ }

f j(a: Str, b: Str)->Str eff [pure] {
  ret a;
}

f bn(path: Str)->Str eff [pure] {
  ret path;
}

f dn(path: Str)->Str eff [pure] {
  ret path;
}

f ext(path: Str)->EO eff [pure] {
  ret None{ };
}
